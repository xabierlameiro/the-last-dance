        "@google-analytics/data": "^3.1.2",
        "@mdx-js/loader": "^2.3.0",
        "@mdx-js/react": "^2.3.0",
        "@next/mdx": "^13.2.4",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "dotenv-load": "^2.0.1",
        "eslint-plugin-next": "^0.0.0",
        "googleapis": "^110.0.0",
        "gray-matter": "^4.0.3",
        "jest": "^28.1.2",
        "jest-environment-jsdom": "^29.3.1",
        "jest-fetch-mock": "^3.0.3",
        "jsdoc-babel": "^0.5.0",
        "next": "^13.4.3-canary.0",
        "next-env": "^1.1.1",
        "next-mdx-remote": "^4.4.1",
        "nodemailer": "^6.9.1",
        "octokit": "^2.0.11",
        "prettier": "^2.8.3",
        "react": "^18.2.0",
        "react-confetti": "^6.1.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.7.1",
        "react-intl": "^6.2.5",
        "swr": "^2.0.2"
    },
    "devDependencies": {
        "@playwright/test": "^1.31.2",
        "@storybook/addon-essentials": "^7.0.0-beta.12",
        "@storybook/addon-interactions": "^7.0.0-beta.12",
        "@storybook/addon-links": "^7.0.0-beta.12",
        "@storybook/addons": "^6.5.14",
        "@storybook/blocks": "^7.0.0-beta.12",
        "@storybook/nextjs": "^7.0.0-beta.12",
        "@storybook/react": "^7.0.0-beta.12",
        "@storybook/testing-library": "^0.0.13",
        "@storybook/theming": "^6.5.14",
        "@types/glob": "^8.0.0",
        "@types/node": "^18.13.0",
        "@types/nodemailer": "^6.4.7",
        "@types/react": "^18.0.28",
        "@types/react-dom": "18.0.9",
        "cross-env": "6",
        "eslint": "^8.30.0",
        "eslint-config-next": "^13.1.6",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-storybook": "^0.6.8",
        "lighthouse": "^10.1.1",
